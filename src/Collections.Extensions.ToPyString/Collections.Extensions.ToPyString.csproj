<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <PackageId>Collections.Extensions.ToPyString</PackageId>
    <Version>1.0.3.2</Version>
    <Authors>Nenad Misic</Authors>
    <ReleaseNotes>Enable converting System.Collections to string in Python format</ReleaseNotes>
    <PackageDescription>.NET System.Collections ToString extension for converting collections to a string in the format that is based on the one in Python</PackageDescription>
    <Description>.NET System.Collections ToString extension for converting collections to a string in the format that is based on the one in Python</Description>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <copyright>Copyright 2020</copyright>
    <PackageTags>system;collections;extensions;tostring;topystring</PackageTags>
    <RepositoryUrl>https://github.com/misicnenad/dotnet-collections-extensions-topystring</RepositoryUrl>
    <PackageIconUrl>https://raw.githubusercontent.com/misicnenad/dotnet-collections-extensions-topystring/master/src/Collections.Extensions.ToPyString/images/string.png</PackageIconUrl>
    <PackageIcon>string.png</PackageIcon>
    <PackageReleaseNotes>Sign the package</PackageReleaseNotes>
    <SignAssembly>true</SignAssembly>
    <AssemblyOriginatorKeyFile>ToPyStringKey.snk</AssemblyOriginatorKeyFile>
  </PropertyGroup>

  <ItemGroup>
    <None Include="images\string.png" Pack="true" PackagePath="\" />
  </ItemGroup>

</Project>
